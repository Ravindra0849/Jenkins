pipeline{
    agent any
    tools{
        jdk 'jdk17'
        terraform 'terraform'
    }
    stages{
        stage('clean Workspace'){
            steps{
                cleanWs()
            }
        }

        stage('checkout from Git'){
            steps{
                git 'https://github.com/Ravindra0849/Jenkins.git'
            }
        }

        stage('Terraform version'){
                steps{
                sh 'terraform --version'
            }
        }
    }
}